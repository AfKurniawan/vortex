        android:layout_height="1dp"
        android:layout_above="@+id/rvDrawer_settings"
        android:layout_gravity="bottom"
        android:background="@color/primary"
    />
    <com.balysv.materialripple.MaterialRippleLayout
        app:mrl_rippleAlpha="0.2"
